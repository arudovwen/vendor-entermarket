{
  "rules": {
    "no-undef": "off",
    "react/no-unknown-property": "off"
  },
  "extends": ["plugin:react/recommended"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 8,
    "requireConfigFile": false,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    },
    "babelOptions": {
      "presets": ["@babel/preset-react"]
    }
  }
}
